  {/* <Badge badgecontent={quantity} color=\"primary\"> */}\n                            <AddShoppingCartOutlined badgecontent={quantity} color=\"primary\"/>\n                        {/* </Badge> */}\n                    </MenuItem>\n                    </Link>\n                </Right>\n            </Wrapper>\n        </Container>\n    )\n}\n\nexport default Navbar;\n","/Users/adhiimacbookair/Desktop/SHOP/client/src/components/Footer.jsx",[],[],"/Users/adhiimacbookair/Desktop/SHOP/client/src/components/Categories.jsx",[],[],"/Users/adhiimacbookair/Desktop/SHOP/client/src/components/Announcement.jsx",[],[],"/Users/adhiimacbookair/Desktop/SHOP/client/src/components/Products.jsx",["125"],[],"import { styled } from \"styled-components\"\nimport { popularProducts } from \"../data\"\nimport { Product } from \"./Product\"\nimport { useEffect, useState } from \"react\"\nimport axios from \"axios\";\n\nconst Container = styled.div`\n    padding: 20px;\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: space-between;\n`\n\nexport const Products = ({ cat, filters, sort }) => {\n  // console.log(cat, filters, sort);\n  const [products, setProducts] = useState([]);\n  const [filteredProducts, setFilteredProducts] = useState([]);\n\n  useEffect(() => {\n    const getProducts = async () => {\n      try {\n        const res = await axios.get(\n          cat\n            ? `http://localhost:3000/api/products?category=${cat}`\n            : \"http://localhost:3000/api/products\"\n        );\n        setProducts(res.data);\n      } catch (err) {\n        console.log(err);\n      }\n    };\n    getProducts();\n  }, [cat]);\n\n  useEffect(() => {\n    cat &&\n      setFilteredProducts(\n        products.filter((item) =>\n          Object.entries(filters).every(([key, value]) =>\n            item[key].includes(value)\n          )\n        )\n      );\n  }, [products, cat, filters]);\n\n  useEffect(() => {\n    if (sort === \"newest\") {\n      setFilteredProducts(prev =>\n        [...prev].sort((a, b) => a.createdAt - b.createdAt)\n      );\n    }else if((sort === \"asc\")){\n      setFilteredProducts((prev)=>\n      [...prev].sort((a, b) => a.price - b.price)\n      );\n    }else{\n      setFilteredProducts((prev)=>\n        [...prev].sort((a, b) => b.price - a.price)\n      );\n    }\n  }, [sort]);\n\n  return (\n    <Container>\n      {cat \n      ? filteredProducts.map(item => (<Product item={item} key={item.id} />)) \n      : products.slice(0,8)\n      .map(item => (<Product item={item} key={item.id} />))}\n    </Container>\n  )\n}\n","/Users/adhiimacbookair/Desktop/SHOP/client/src/components/Newsletter.jsx",[],[],"/Users/adhiimacbookair/Desktop/SHOP/client/src/components/Slider.jsx",[],[],"/Users/adhiimacbookair/Desktop/SHOP/client/src/data.js",[],[],"/Users/adhiimacbookair/Desktop/SHOP/client/src/components/Product.jsx",[],[],"/Users/adhiimacbookair/Desktop/SHOP/client/src/components/CategoryItem.jsx",[],[],"/Users/adhiimacbookair/Desktop/SHOP/client/src/requestMethods.js",[],[],"/Users/adhiimacbookair/Desktop/SHOP/client/src/redux/store.js",[],[],"/Users/adhiimacbookair/Desktop/SHOP/client/src/redux/cartRedux.js",[],[],"/Users/adhiimacbookair/Desktop/SHOP/client/src/pages/Success.jsx",["126"],[],"\nimport { useLocation } from 'react-router-dom';\n\nconst Success = () => {\n  const location = useLocation(); \n  return (\n    <div>Successful</div>\n  )\n}\n\nexport default Success;",{"ruleId":"127","severity":1,"message":"128","line":7,"column":10,"nodeType":"129","messageId":"130","endLine":7,"endColumn":18},{"ruleId":"127","severity":1,"message":"131","line":2,"column":43,"nodeType":"129","messageId":"130","endLine":2,"endColumn":48},{"ruleId":"127","severity":1,"message":"132","line":2,"column":50,"nodeType":"129","messageId":"130","endLine":2,"endColumn":78},{"ruleId":"127","severity":1,"message":"133","line":2,"column":10,"nodeType":"129","messageId":"130","endLine":2,"endColumn":25},{"ruleId":"127","severity":1,"message":"